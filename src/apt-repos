#!/bin/bash
#
# wrapper for calling ./apt_repos.py with the following tasks:
# - suppress some unwanted stdout-output created by underlying
#   apt-libs
#

$(dirname $(realpath $0))/apt_repos.py $@ \
    | grep -v "Reading package lists..." \
    | grep -v "Building dependency tree..." \
    | grep -v "Abh√§ngigkeitsbaum wird aufgebaut..." \
    | grep -v "Paketlisten werden gelesen"
